	<queryString>
		<![CDATA[Select
  entidade.nome,
  conta.datacriacao,
  subconta.datavencimento,
  subconta.valorparcela
from
  conta, subconta, entidade
where
  conta.id = subconta.contaid
  and
  conta.entidadeId = entidade.id
  and
  conta.status = $P{status}
  and
  subConta.situacao = $P{situacao}
  and
  $P!{where_clause}
order by
  1,2]]>
	</queryString>
