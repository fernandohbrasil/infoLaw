	<queryString>
		<![CDATA[Select
  entidade.nome,
  conta.id,
  conta.datacriacao,
  subconta.sequencia,
  subconta.datapagamento,
  subconta.datavencimento,
  subconta.valorparcela
from
  conta, subconta, entidade
where
  conta.id = subconta.contaid
  and
